<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph"
            app:startDestination="@id/mainFragment">

    <fragment android:id="@+id/mainFragment" android:name="retanar.test.chi.MainFragment" android:label="MainFragment">
        <action android:id="@+id/action_mainFragment_to_countingFragment" app:destination="@id/countingFragment"/>
    </fragment>
    <fragment android:id="@+id/countingFragment" android:name="retanar.test.chi.CountingFragment"
              android:label="fragment_counting" tools:layout="@layout/fragment_counting">
        <argument
            android:name="counter"
            app:argType="integer"
            android:defaultValue="0"/>
    </fragment>
</navigation>