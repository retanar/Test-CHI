<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
            app:startDestination="@id/mainFragment">

    <fragment android:id="@+id/mainFragment" android:name="retanar.test.chi.presentation.MainFragment"
              android:label="fragment_main" tools:layout="@layout/fragment_main">
        <action android:id="@+id/action_mainFragment_to_addUserFragment" app:destination="@id/addUserFragment"/>
        <action android:id="@+id/action_mainFragment_to_userDetailsFragment" app:destination="@id/userDetailsFragment"/>
    </fragment>
    <fragment android:id="@+id/addUserFragment" android:name="retanar.test.chi.presentation.AddUserFragment"
              android:label="fragment_add_user" tools:layout="@layout/fragment_add_user"/>
    <fragment android:id="@+id/userDetailsFragment" android:name="retanar.test.chi.UserDetailsFragment"
              android:label="fragment_user_details" tools:layout="@layout/fragment_user_details"/>
</navigation>